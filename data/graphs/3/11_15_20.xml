<?xml version="1.0" encoding="UTF-8"?>
<sdf3 version="1.0" type="sdf" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<applicationGraph name="app">
		<sdf name="tmp" type="Example">
			<actor name="a1" type="A">
				<port name="in1" type="in" rate="11"/>
				<port name="out1" type="out" rate="11"/>
			</actor>
			<actor name="a2" type="A">
				<port name="in2" type="in" rate="15"/>
				<port name="out2" type="out" rate="15"/>
			</actor>
			<actor name="a3" type="A">
				<port name="in3" type="in" rate="20"/>
				<port name="out3" type="out" rate="20"/>
			</actor>
			<channel name="d1" srcActor="a1" srcPort="out1" dstActor="a2" dstPort="in2" size="1" initialTokens="0"/>
			<channel name="d2" srcActor="a2" srcPort="out2" dstActor="a3" dstPort="in3" size="1" initialTokens="0"/>
			<channel name="d3" srcActor="a3" srcPort="out3" dstActor="a1" dstPort="in1" size="1" initialTokens="40"/>
		</sdf>
		<sdfProperties>
			<actorProperties actor="a1">
				<processor type="cluster_0" default="true">
					<executionTime time="1"/>
				</processor>
			</actorProperties>
			<actorProperties actor="a2">
				<processor type="cluster_0" default="true">
					<executionTime time="1"/>
				</processor>
			</actorProperties>
			<actorProperties actor="a3">
				<processor type="cluster_0" default="true">
					<executionTime time="1"/>
				</processor>
			</actorProperties>
		</sdfProperties>
	</applicationGraph>
</sdf3>
